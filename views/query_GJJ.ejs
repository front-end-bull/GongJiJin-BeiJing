        function changeType(){
            var select =  $('#type')
            var val = select.val()
            var label = '请输入你的'+$('#type').find("option:selected").text()
            if(val==2||val==3||val==4){
                label+='号'
            }
            $('#type_name').attr('placeholder',label)
        }

        function agree(){
            isChecked = !isChecked
